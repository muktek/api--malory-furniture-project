{
  "name": "webpack-es6",
  "version": "1.0.0",
  "description": "starter kit for webpack-es6",
  "main": "app.js",
  "scripts": {
    "init-project" : "npm run remove-origin && npm run set-project-name && install",
    "remove-origin" : "git remote remove origin || true",
    "start": "node --use_strict server.js",
    "go": "npm-run-all --parallel dev-server",
    "dev-server": "NODE_ENV=development nodemon server.js --watch server.js --watch src-server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "set-project-name": "node ./src-server/cli/setProjectName.js",
    "seed:dev": "NODE_ENV=development node --use_strict ./src-server/db/seed.js",
    "seed:prod": "node --use_strict ./src-server/db/seed.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "0.15.3",
    "body-parser": "1.16.1",
    "cors": "2.8.3",
    "express": "4.14.0",
    "mongoose": "4.8.3"
  },
  "devDependencies": {
    "chalk": "1.1.3",
    "npm-run-all": "4.0.1",
    "prompt": "1.0.0",
    "nodemon": "1.11.0"

  }
}
